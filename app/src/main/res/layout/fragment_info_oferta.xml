<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InfoOferta">

    <View
        android:id="@+id/_bg__informacion_de_una_propuesta_ek2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/_bg__informacion_de_una_propuesta_ek2_shape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/informacion_de_la_propuesta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:text="Informacion de la propuesta"
        android:textColor="@color/vista_de_respuestas_color"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_6_ek4"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:background="@drawable/rectangle_6_ek4_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/image_6_ek2"
        android:layout_width="68dp"
        android:layout_height="63dp"
        android:layout_marginEnd="120dp"
        android:src="@drawable/image_6_ek2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/_avatar_1_ek2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rectangle_6_ek4"
        app:layout_constraintVertical_bias="0.47" />

    <ImageButton
        android:id="@+id/_avatar_1_ek2"
        android:layout_width="61dp"
        android:layout_height="64dp"
        android:src="@drawable/_avatar_1_ek2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/_image_5_ek3"
        app:layout_constraintStart_toStartOf="@id/image_6_ek2"
        app:layout_constraintTop_toTopOf="@+id/rectangle_6_ek4" />

    <ImageButton
        android:id="@+id/_image_5_ek3"
        android:layout_width="68dp"
        android:layout_height="63dp"
        android:layout_marginStart="300dp"
        android:src="@drawable/_image_5_ek3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rectangle_6_ek4" />

    <ImageButton
        android:id="@+id/_image_5_ek2"
        android:layout_width="97.35dp"
        android:layout_height="64dp"
        android:layout_marginBottom="120dp"
        android:src="@drawable/_image_5_ek2"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_6_ek4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>