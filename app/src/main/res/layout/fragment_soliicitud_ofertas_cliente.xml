<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SolicitudesDeCliente">

    <View
        android:id="@+id/_bg__pantalla_de_bienvenida_ek2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/_bg__pantalla_de_bienvenida_ek2_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/VwVista"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:background="@drawable/rectangle_7_shape"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_6_ek4"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/tvSolicitudes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bandeja de Solicitudes"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="35sp"
        android:layout_marginTop="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/tvNombreOferta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="185dp"
        android:textSize="20sp"
        android:gravity="start"
        android:text="Nombre Oferta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvNombreEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="35dp"
        android:gravity="start"
        android:textSize="20sp"
        android:text="Nombre Empresa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNombreOferta" />

    <TextView
        android:id="@+id/tvJefeEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="35dp"
        android:gravity="start"
        android:textSize="20sp"
        android:text="Representante"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNombreEmpresa" />

    <TextView
        android:id="@+id/tvCorreoEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="35dp"
        android:textSize="20sp"
        android:gravity="start"
        android:text="Correo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvJefeEmpresa" />

    <TextView
        android:id="@+id/tvDirecEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:textSize="20sp"
        android:layout_marginTop="35dp"
        android:gravity="start"
        android:text="Dirrecion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCorreoEmpresa" />

    <TextView
        android:id="@+id/tvRespuesta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:textSize="20sp"
        android:layout_marginTop="35dp"
        android:gravity="start"

        android:text="Repuesta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDirecEmpresa" />

    <TextView
        android:id="@+id/tvReferencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:textSize="20sp"
        android:layout_marginTop="35dp"
        android:gravity="start"
        android:text="Referencia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRespuesta" />

    <View
        android:id="@+id/rectangle_6_ek4"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:background="@drawable/rectangle_6_ek4_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/IvInicio"
        android:layout_width="68dp"
        android:layout_height="63dp"
        android:src="@drawable/image_6_ek2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/IvMail"
        app:layout_constraintStart_toStartOf="@id/IvPersona"
        app:layout_constraintTop_toTopOf="@+id/rectangle_6_ek4"
        />

    <ImageView
        android:id="@+id/IvPersona"
        android:layout_width="61dp"
        android:layout_height="64dp"
        android:src="@drawable/_avatar_1_ek2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rectangle_6_ek4"
        android:layout_marginStart="30dp"/>

    <ImageView
        android:id="@+id/IvMail"
        android:layout_width="68dp"
        android:layout_height="63dp"
        android:layout_marginStart="300dp"
        android:src="@drawable/_image_5_ek3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rectangle_6_ek4" />

</androidx.constraintlayout.widget.ConstraintLayout>